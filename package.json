{
  "name": "fr-starter",
  "version": "0.1.0",
  "description": "Fulcrum project starter",
  "main": "./lib/starter.js",
  "bin": {
    "fr-starter": "./bin/index.js"
  },
  "scripts": {
    "start": "./lib/starter.js",
    "lint": "eslint lib/ --fix",
    "pretty-quick": "pretty-quick",
    "prettier": "prettier --write '**/*.js'"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run pretty-quick",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "O.Bespalko",
  "license": "MIT",
  "dependencies": {
    "cli-spinner": "^0.2.10",
    "inquirer": "^7.0.0",
    "node-emoji": "^1.10.0",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  }
}
